{
	"name": "ng-porcelain-src",
	"version": "9.0.7",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build ngPorcelain --prod",
		"test": "ng test ngPorcelain",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"storybook": "start-storybook",
		"format-staged": "pretty-quick --staged",
		"build-sync-tool": "cd tools/check-versions/ && tsc",
		"build-readme-tool": "cd tools/readme-builder/ && tsc",
		"build-variables-tool": "cd tools/get-variables/ && tsc",
		"build-tools": "npm run build-sync-tool && npm run build-readme-tool && npm run build-variables-tool",
		"clean-tools": "rm -rf tools/readme-builder/dist/ tools/check-versions/dist/ tools/get-variables/dist/",
		"build-readme": "node tools/readme-builder/dist/index.js -t \"Porcelain for Angular\" -c readme.json -o \"projects/ng-porcelain/README.md\" -p \"projects/ng-porcelain/package.json\" ",
		"sync-packages": "node tools/check-versions/dist/index.js --fix --primary \"projects/ng-porcelain/package.json\" --secondary \"package.json\" ",
		"get-variables": "node tools/get-variables/dist/index.js --input \"projects/ng-porcelain/package.json\" --prefix \"package__\" --delimiter \"__\" ",
		"pre-commit": "npm run format-staged && npm run build-sync-tool && npm run sync-packages",
		"create-package": "npm run build-tools && npm run build-readme && npm run sync-packages && npm run build "
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run pre-commit"
		}
	},
	"private": true,
	"dependencies": {
		"@angular/cdk": "^9.0.0",
		"@angular/animations": "^9.0.0",
		"@angular/common": "^9.0.0",
		"@angular/core": "^9.0.0",
		"@angular/forms": "^9.0.0",
		"@angular/platform-browser": "^9.0.0",
		"@angular/platform-browser-dynamic": "^9.0.0",
		"@angular/router": "^9.0.0",
		"@fortawesome/angular-fontawesome": "^0.6.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.30",
		"@fortawesome/free-regular-svg-icons": "^5.13.1",
		"@fortawesome/free-solid-svg-icons": "^5.14.0",
		"@w11k/angular-sticky-things": "~1.3.0",
		"angular-mydatepicker": "^0.10.2",
		"chalk": "^4.1.0",
		"core-js": "^3.6.5",
		"date-fns": "^1.30.1",
		"lodash-es": "^4.0.0",
		"moment": "^2.27.0",
		"reset-css": "^5.0.1",
		"rxjs": "~6.6.2",
		"sprintf-js": "^1.1.2",
		"tslib": "^1.10.0",
		"uuid": "^3.4.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.901.12",
		"@angular-devkit/build-ng-packagr": "~0.901.12",
		"@angular/cli": "~9.1.12",
		"@angular/compiler": "^9.1.12",
		"@angular/compiler-cli": "^9.1.12",
		"@angular/language-service": "^9.1.12",
		"@babel/core": "^7.10.3",
		"@storybook/addon-actions": "~6.0.13",
		"@storybook/addon-backgrounds": "~6.0.13",
		"@storybook/addon-knobs": "~6.0.13",
		"@storybook/addon-notes": "~5.3.19",
		"@storybook/addon-storysource": "~6.0.13",
		"@storybook/angular": "~6.0.13",
		"@types/date-fns": "^2.6.0",
		"@types/faker": "^4.1.12",
		"@types/flat": "^5.0.1",
		"@types/jasmine": "2.8.6",
		"@types/jasminewd2": "2.0.3",
		"@types/node": "^12.11.1",
		"@types/semver": "^7.3.4",
		"@types/sprintf-js": "^1.1.2",
		"@types/yargs": "^15.0.5",
		"babel-loader": "^8.1.0",
		"codelyzer": "^5.1.2",
		"faker": "^4.1.0",
		"fast-lorem-ipsum": "^1.2.0",
		"flat": "^5.0.2",
		"husky": "^1.3.1",
