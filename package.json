{
	"name": "ng-porcelain-src",
	"version": "9.3.2",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build ngPorcelain --prod",
		"test": "ng test ngPorcelain",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"storybook": "start-storybook",
		"format-staged": "pretty-quick --staged",
		"build-readme": "npx build-readme -t \"Porcelain for Angular\" -c readme.json -o \"projects/ng-porcelain/README.md\" -p \"projects/ng-porcelain/package.json\" ",
		"sync-packages": "npx sync-packages --fix --primary \"projects/ng-porcelain/package.json\" --secondary \"package.json\" ",
		"get-variables": "npx set-azdo-variables --input \"projects/ng-porcelain/package.json\" --prefix \"package__\" --delimiter \"__\" ",
		"pre-commit": "npm run format-staged && npm run sync-packages",
		"create-package": "npm run build-readme && npm run sync-packages && npm run build "
	},
	"bin": {
		"sync-packages": "tools/sync-packages/bin/run",
		"set-azdo-variables": "tools/set-azdo-variables/bin/run",
		"build-readme": "tools/build-readme/bin/run",
		"get-latest-version": "tools/get-latest-version/bin/run"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run pre-commit"
		}
	},
